{
  "name": "happysir/distributed-lock",
  "description": "A distributed lock based on PHP and redis(swoft)",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "happysir",
      "email": "luotao954@gmail.com"
    }
  ],
  "require": {
    "php": ">7.2",
    "ext-redis": "*",
    "ramsey/uuid": "3.8.0"
  },
  "require-dev": {
    "swoft/swoole-ide-helper": "dev-master",
    "swoft/framework": "~2.0.0",
    "swoft/redis": "~2.0.0",
    "symfony/expression-language": "~4.3",
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "Happysir\\Lock\\": "src/"
    },
    "files": [
      "./src/Helper/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Happysir\\Lock\\Test\\": "test/",
      "Happysir\\Lock\\Unit\\": "test/unit/",
      "Happysir\\Lock\\Testing\\": "test/testing/"
    }
  }
}
